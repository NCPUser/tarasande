{
  "schemaVersion": 1,
  "id": "tarasande-mod-fixes",
  "version": "${version}",

  "name": "tarasande Mod Fixes",
  "description": "tarasande package to provide fixes for other mods",
  "authors": [
    "Sumandora/Johannes",
    "EnZaXD/FlorianMichael"
  ],
  "contact": {
    "homepage": "https://github.com/tarasandeDevelopment/",
    "sources": "https://github.com/tarasandeDevelopment/tarasande"
  },

  "license": "GPL-3.0",
  "icon": "assets/tarasande-mod-fixes/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "su.mandora.tarasande_mod_fixes.TarasandeModFixes"
    ]
  },
  "mixins": [
    "tarasande-mod-fixes.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "minecraft": "~1.19",
    "java": ">=17",
    "tarasande": "*"
  },
  "custom": {
    "modmenu": {
      "parent": "tarasande"
    }
  }
}
