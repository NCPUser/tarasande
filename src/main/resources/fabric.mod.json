{
  "schemaVersion": 1,
  "id": "tarasande",
  "version": "${version}",
  "name": "tarasande",
  "description": "Tarasande is a fabric mod designed to enhance and improve gameplay",
  "authors": [
    "Sumandora/Johannes"
  ],
  "contact": {
    "sources": "https://github.com/Sumandora/tarasande"
  },
  "license": "All rights reserved",
  "icon": "assets/tarasande/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "su.mandora.tarasande.Entrypoint::onInitialize"
      }
    ]
  },
  "mixins": [
    "tarasande.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "minecraft": "1.18.1",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.7.0+kotlin.1.6.0"
  }
}
